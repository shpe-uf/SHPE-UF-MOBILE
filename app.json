{
  "expo": {
    "name": "SHPE UF",
    "slug": "SHPEMobile",
    "privacy": "public",
    "description": "App for SHPE UF members to redeem points",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F6F6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@codler/react-native-keyboard-aware-scroll-view"
          ]
        }
      }
    },
    "githubUrl": "https://github.com/shpe-uf/SHPE-UF-MOBILE"
  }
}
